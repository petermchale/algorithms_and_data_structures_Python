   "source": [
    "def factorial_tailrecursive(n, running_total=1):\n",
    "    if n == 1:\n",
    "        return running_total\n",
    "    return factorial_tailrecursive(n-1, n*running_total)\n",
    "\n",
    "factorial_tailrecursive(5)"
   ]
